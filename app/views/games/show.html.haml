.screen-container
  :javascript
    $(function(){
      WEBHACK.screen = WEBHACK.create_screen("div.screen-container", {linefeed_handling : "#{::WEBHACK_CONFIG.linefeed_handling}"});

      WEBHACK.listener = WEBHACK.create_listener("#{game_dungeon_url}");
      WEBHACK.listener.start();

    });
  = hidden_field_tag :authenticity_token, form_authenticity_token


